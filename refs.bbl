    \field{title}{Drier, darker and more fertile: 140 years of plant habitat
  change driven by land-use intensification}
    \field{volume}{32}
    \field{journaltitle}{Journal of Vegetation Science}
    \field{month}{07}
    \field{year}{2021}
  \endentry

