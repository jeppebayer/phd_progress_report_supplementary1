    \field{pages}{1411\bibrangedash 1424}
    \field{title}{Overcoming confusion and stigma in habitat fragmentation
  research}
    \field{volume}{99}
    \field{journaltitle}{Biological Reviews}
    \field{month}{08}
    \field{year}{2024}
  \endentry

